<?xml version="1.0"?>
<configuration>
  <system.web>
    <authorization>
       <deny users="?"/>
      <deny users="RegisteredUser"/>
      <allow roles="Admin"/>
    </authorization>
    <customErrors mode="Off"/>
    <compilation debug="true"/>
  </system.web>
<location path="index.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
<location path="Edit.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin" />
        <deny users="*" />
      </authorization>
    </system.web>
  </location>
</configuration>
